<!doctype html>
<html class="no-js" lang="{{ request.locale.iso_code }}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta name="theme-color" content="">
    <link rel="canonical" href="{{ canonical_url }}">
    <link rel="preconnect" href="https://cdn.shopify.com" crossorigin>

    {%- if settings.favicon != blank -%}
      <link rel="icon" type="image/png" href="{{ settings.favicon | img_url: '32x32' }}">
    {%- endif -%}

    {%- unless settings.type_header_font.system? and settings.type_body_font.system? -%}
      <link rel="preconnect" href="https://fonts.shopifycdn.com" crossorigin>
    {%- endunless -%}

    <title>
      {{ page_title }}
      {%- if current_tags %} &ndash; tagged "{{ current_tags | join: ', ' }}"{% endif -%}
      {%- if current_page != 1 %} &ndash; Page {{ current_page }}{% endif -%}
      {%- unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless -%}
    </title>

    {% if page_description %}
      <meta name="description" content="{{ page_description | escape }}">
    {% endif %}

    {% render 'meta-tags' %}

    <script src="{{ 'global.js' | asset_url }}" defer="defer"></script>
    {% include 'globo.filter.script' %}


    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.3/css/bootstrap.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/Swiper/5.4.5/css/swiper.css">
    <link rel="stylesheet" href="https://npmcdn.com/js-offcanvas/dist/_css/minified/js-offcanvas.css">

    {{ 'style.css' | asset_url | stylesheet_tag }}
    {{ 'media.css' | asset_url | stylesheet_tag }}

  <!--
      <script src="https://code.jquery.com/jquery-2.2.4.min.js" integrity="sha256-BbhdlvQf/xTY9gja0Dq3HiwQF8LaCRTXxZKRutelT44=" crossorigin="anonymous"></script>
    -->
    <script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.3/js/bootstrap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/protonet-jquery.inview/1.1.2/jquery.inview.min.js"></script>


    {{ 'jquery.aniview.min.js' | asset_url | script_tag }}

    <script src="https://cdnjs.cloudflare.com/ajax/libs/Swiper/5.4.5/js/swiper.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/iScroll/5.2.0/iscroll.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js"></script>
    {{ 'main.js' | asset_url | script_tag }}

    <script>document.documentElement.className = document.documentElement.className.replace('no-js', 'js');</script>
    <div style="visibility: hidden">
      {{ content_for_header }}
    </div>
    <style>
    .header-body {z-index: 1}
    .header-topbar .site-tools>ul>li>a>i {font-size: 28px;}
    #off-canvas-top {background-color: #fff !important; padding:30px 0 60px; height: auto}
    #off-canvas-top .nav-content {width: 90%; margin:30px auto;}
    #off-canvas-top .input-group {flex-wrap: nowrap;}
    #off-canvas-top .input-group .btn {border:none; font-size: 16px;}
    #off-canvas-top .input-search {border: none; border-bottom: 1px solid #333; padding: 12px 45px 12px 12px; outline: none; box-shadow: none; font-size: 16px; flex-grow: 1}
    #off-canvas-top .input-group-append {position: absolute; bottom: 2px; right: -10px;}
    #off-canvas-top .input-search:focus {outline: none;}
    #off-canvas-top .l-wrapper {width: 90%; margin:0 auto; overflow: hidden;}
    #off-canvas-right-push, #main-nav-mobile {background-color: #fff}
    #off-canvas-right-push ul li:last-child {position: fixed; bottom: 0; width: 100%; background-color: #f1f1f1; border-top: 1px solid #ddd}
    #off-canvas-right-push .st-list-cart-items {height: calc(100vh - 210px); overflow-y: scroll; padding:0;}
    .nav-title {font-size: 19px; font-weight: normal; padding: 20px 0; color: #333;}
    .js-offcanvas-close {position: absolute; top: 15px; right: 15px; color: #333;}
    .js-offcanvas-close:hover {color: var(--main-color)}
    .st-cart-item .st-product_remove>a>i {font-size: 14px !important;}
    .c-offcanvas--right {width: 27em;}
    .c-offcanvas-bg.c-offcanvas-bg--push.c-offcanvas-bg--right.is-open {transform: translate3d(-27em,0,0)}
    .c-offcanvas-bg.is-animating, .c-offcanvas-bg.is-open {background-color: rgba(0,0,0,.4)}
    #cartWrapper ul {list-style: none; padding:0;}
    #cartWrapper ul li {margin-left: 0;}
    #cartWrapper li a {color: #333;}
    #cartWrapper .nav-title {padding:15px;}
    #main-nav-mobile a {color: #333;}

.hamburger-menu {
  display: inline-block;
  height: 43px;
  transition: all 0.3s ease-in-out;
}
.hamburger-menu .menu-item {
  background: #fff;
  display: block;
  height: 2px;
  margin: 0 0 10px;
  transition: all 0.3s ease-in-out;
  width: 30px;
}
    @media (max-width: 991px) {.drawer-hamburger {display: block;}}
    @media (max-width: 768px) {
    .search-input {height: auto;}
    #off-canvas-top {padding-bottom: 0;}
    }
    </style>
  </head>
  <body>

    <div id="preloader"></div>
    {% section 'header' %}
    <main role="main c-offcanvas-content-wrap" class="content" role="main">
    <div class="c-page l-wrapper u-pos-relative">
      {{ content_for_layout }}
    </div>
    </main>

    {% section 'footer' %}

    <ul hidden>
      <li id="a11y-refresh-page-message">{{ 'accessibility.refresh_page' | t }}</li>
    </ul>

    <script>
      window.routes = {
        cart_add_url: '{{ routes.cart_add_url }}',
        cart_change_url: '{{ routes.cart_change_url }}',
        cart_update_url: '{{ routes.cart_update_url }}',
        predictive_search_url: '{{ routes.predictive_search_url }}'
      };

      window.cartStrings = {
        error: `{{ 'sections.cart.cart_error' | t }}`,
        quantityError: `{{ 'sections.cart.cart_quantity_error_html' | t }}`
      }

      window.variantStrings = {
        addToCart: `{{ 'products.product.add_to_cart' | t }}`,
        soldOut: `{{ 'products.product.sold_out' | t }}`,
        unavailable: `{{ 'products.product.unavailable' | t }}`,
      }

      window.accessibilityStrings = {
        shareSuccess: `{{ 'general.share.success_message' | t }}`,
      }
    </script>

    {%- if settings.predictive_search_enabled -%}
      <script src="{{ 'predictive-search.js' | asset_url }}" defer="defer"></script>
    {%- endif -%}
  </body>
</html>
{% include 'globo.menu.script' %}
{% include 'globo.filter.action' %}
